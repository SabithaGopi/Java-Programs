package Interfaces;

public interface Bike {
int a=9;
	
	public String getRegistrationBike();
	public void setRegistrationBike(String s);
	public double calculateMilage();
}


package Interfaces;

public interface Car extends EVSuv {
	public String getRegistrationCar();
	public void setRegistrationCar(String s);
	public double calculateEVCharge();

}

package Interfaces;

public abstract class ComplianceVehicle {

	public abstract void newRules();
	public abstract void changeRules();

	
}

package Interfaces;

public interface EVSuv {

	
	public void EVDetails();
}


package Interfaces;

public interface Truck {

	public String getRegistrationTruck();
	public void setRegistrationTruck(String s);
	public double calculateRoadTax();


}

package Interfaces;

public abstract class Vehicle extends ComplianceVehicle {
	
int a;
	
	public abstract String getRegistration();
	public abstract void setRegistration(String s);
	public abstract String getTransmissionType();
	public abstract void setTransmissionType(String s);
	public abstract String getMake();
	public abstract void setMake(String s);
	
	public void recallsCount(int a)
	{
		System.out.println("Recalls:"+a);
	}

	/*public static void main(String[] arg)
	{
		Vehicle v = new VehicleImpl();
		v.recallsCount(0);
		
	}*/

}

