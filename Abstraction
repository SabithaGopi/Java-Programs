package abstractions;

public abstract class Vehicle {
	
int a;
	
	public abstract String getRegistration();
	public abstract void setRegistration(String s);
	public abstract String getTransmissionType();
	public abstract void setTransmissionType(String s);
	public abstract String getMake();
	public abstract void setMake(String s);
	
	public void recallsCount(int a)
	{
		System.out.println("Recalls:"+a);
	}

	/*public static void main(String[] arg)
	{
		Vehicle v = new VehicleImpl();
		v.recallsCount(0);
		
	}*/

}


